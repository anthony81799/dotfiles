{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#2B2B2B",
          "foreground": "#0377C8",
          "style": "plain",
          "template": " {{.Icon}} \ue0b1",
          "type": "os"
        },
        {
          "background": "#2B2B2B",
          "foreground": "#0377C8",
          "style": "plain",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "background": "#2B2B2B",
          "foreground": "#0377C8",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#2B2B2B",
          "foreground": "#4A9207",
          "style": "plain",
          "template": "<#0377C8>\ue0b1 </>{{ .HEAD }} ",
          "type": "git"
        },
        {
          "background": "transparent",
          "foreground": "#2B2B2B",
          "style": "plain",
          "template": "\ue0b0",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "segments": [
        {
          "background": "#2B2B2B",
          "foreground": "#4A9207",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#FF5252{{ end }}"
          ],
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf00c{{ end }} <#0377C8>\ue0b3</>",
          "type": "status"
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "background": "#2B2B2B",
          "foreground": "#C4a000",
          "foreground_templates": [
            "{{if gt .Ms 5000 }}#FF5252{{ end }}"
          ],
          "template": "{{ .FormattedMs }} <#0377C8>\ue0b3</>",
          "properties": {
            "threshold": 10,
            "style": "austin"
          }
        },
        {
          "background": "#2B2B2B",
          "foreground": "#4A9207",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "template": "{{ .CurrentDate | date .Format }} \uf017 ",
          "type": "time"
        }
      ],
      "type": "rprompt"
    }
  ],
  "transient_prompt": {
    "background": "#2B2B2B",
    "foreground": "#0377C8",
    "style": "diamond",
    "template": " {{ .Folder }}<#2B2B2B,transparent>\ue0b0</>"
  },
  "version": 2
}